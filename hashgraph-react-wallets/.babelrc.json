{
    "presets": [
        "@babel/preset-typescript",
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "@babel/plugin-transform-runtime",
            {
                "absoluteRuntime": false,
                "corejs": false,
                "helpers": true,
                "regenerator": true
            }
        ],
        "@babel/plugin-proposal-private-methods"
    ],
    "ignore": [
        "**/types.ts"
    ]
}